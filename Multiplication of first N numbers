#include <stdio.h>

int main() 
{
    int n, i, product;
    printf("Multiplicaton of first N numbers.\nInsert number: ");
    scanf("%d", &n);
    
    product=1;
    i=1;
    while(i<=n)
    {
        product*=i;
        printf("Factorial(%d)=%d\n", i, product);
        i++;
    }
    printf("Result: %d", product);
    return 0;
}
